<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
        <RootNamespace>JsonRpcClient</RootNamespace>
        <PackageId>JsonRpcClient</PackageId>
        <Version>3.0.0</Version>
        <Authors>Matthew Burkard</Authors>
        <Description>Provides classes for creating JSON RPC 2.0 clients in C#.</Description>
        <PackageProjectUrl>https://gitlab.com/mburkard/json-rpc-2.0-client</PackageProjectUrl>
        <RepositoryUrl>https://gitlab.com/mburkard/json-rpc-2.0-client</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Copyright>Copyright Â© 2021 Matthew Burkard</Copyright>
        <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
        <PackageTags>jsonrpc</PackageTags>
        <LangVersion>9</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
      <PackageReference Include="System.Net.Http" Version="4.3.4" />
    </ItemGroup>

</Project>
